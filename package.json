{
  "name": "react-style-reset",
  "version": "1.3.1",
  "description": "An object-style CSS Reset for React",
  "license": "MIT",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "homepage": "https://github.com/iiroj/react-style-reset",
  "repository": "github:iiroj/react-style-reset",
  "bugs": "https://github.com/iiroj/react-style-reset/issues",
  "main": "index.js",
  "module": "index.mjs",
  "browser": "index.umd.js",
  "files": [
    "/*.d.ts",
    "/*.js",
    "/*.mjs"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "parcel example/index.html",
    "lint": "eslint --ext .js --ext .ts .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "watch": "rollup -cw",
    "build": "rollup -c",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run typecheck && npm run build"
    }
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.2",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "commitlint": "^9.1.2",
    "emotion": "^10.0.27",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.26.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "standard-version": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "css",
    "emotion",
    "normalize",
    "react",
    "reset",
    "style",
    "styled-components"
  ]
}
